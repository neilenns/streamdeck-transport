{
  "version": "1.0.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "streamdeck-transport",
  "author": "Neil Enns",
  "module": "dist/streamdeck-transport.esm.js",
  "size-limit": [
    {
      "path": "dist/streamdeck-transport.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/streamdeck-transport.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^11.1.4",
    "husky": "^9.1.4",
    "size-limit": "^11.1.4",
    "tsdx": "^0.14.1",
    "tslib": "^2.6.3",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@elgato/streamdeck": "^0.3.0",
    "logform": "^2.6.1",
    "triple-beam": "^1.4.1",
    "winston-transport": "^4.7.1"
  }
}
